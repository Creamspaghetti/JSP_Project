<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Order">
	<select id="getProducts" parameterType="String"
		resultType="mydto">
		SELECT * FROM PRODUCTS WHERE product_id = #{product_id}
	</select>
	<insert id="insertOrderedList" parameterType="hashmap">
		INSERT INTO ORDERED (ORDERED_NUM, USER_ID, PRODUCT_ID, ordered_date)
		VALUES (ordered_seq.nextval, #{user_id}, #{product_id}, SYSDATE)
	</insert>
</mapper>