<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="MyMapper">
    <!-- 전체 DB 조회후 productList.jsp에서 표시 -->
    <select id="getAllProducts" resultType="myproductsdto">
        SELECT * FROM Products
    </select>
    
    <!-- refund.jsp에 DB에서 조회한 데이터 가지고 오기 -->
    <select id="getRefundedProducts" parameterType="string" resultType="myrefunddto">
    	SELECT * FROM ORDERED
    	<!-- WHERE ORDER_NUM = #{orderNum} -->
    	<!-- 지금은 1 -->
    </select>
    
    
</mapper>
