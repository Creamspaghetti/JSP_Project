<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="Order">
  <select id="getById" parameterType="string" resultType="userdto" >
 		SELECT * FROM USERS WHERE user_name = #{user_name}
 	</select>
 	<select id="getproductidByMyProducts" parameterType="string" resultType="myproductsdto" >
 		SELECT * FROM PRODUCTS WHERE product_id = #{product_id}
 	</select>
 	<select id="getproductidByCart" parameterType="string" resultType="cartdto" >
 		SELECT * FROM PRODUCTS WHERE product_id = #{product_id}
 	</select>

  </mapper>